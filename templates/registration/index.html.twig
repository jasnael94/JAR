{# templates/registration/register.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <h1>Inscription</h1>

    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        <button class="btn">{{ form.submit.vars.label }}</button>
    {{ form_end(form) }}

    {# <p>Déjà un compte ? <a href="{{ path('app_login') }}">Connectez-vous ici</a></p> #}
{% endblock %}

